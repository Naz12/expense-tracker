{
  "name": "nextjs-fullstack-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run kill-port && npm run db:setup && cross-env DATABASE_URL=file:./prisma/dev.db next dev --turbopack --port 3000",
    "dev:clean": "cross-env DATABASE_URL=file:./prisma/dev.db next dev --turbopack --port 3000",
    "build": "next build --turbopack",
    "start": "cross-env DATABASE_URL=file:./prisma/dev.db next start --port 3000",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:setup": "cross-env DATABASE_URL=file:./prisma/dev.db npm run db:generate && cross-env DATABASE_URL=file:./prisma/dev.db npm run db:push && cross-env DATABASE_URL=file:./prisma/dev.db npm run db:seed",
    "db:push": "cross-env DATABASE_URL=file:./prisma/dev.db prisma db push",
    "db:generate": "cross-env DATABASE_URL=file:./prisma/dev.db prisma generate",
    "db:seed": "cross-env DATABASE_URL=file:./prisma/dev.db tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && prisma db seed",
    "kill-port": "npx kill-port 3000",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.6.0",
    "@trpc/next": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.546.0",
    "next": "15.5.5",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "kill-port": "^2.0.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
